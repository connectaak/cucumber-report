{
  "manifest_version": 3,
  "name": "Cucumber",
  "version": "1.0.0",
  "description": "A basic example extension with only required keys",
 "icons": {
    "16": "favicon.ico",
    "48": "logo192.png",
    "128": "logo512.png"
  },
  "background": {
    "service_worker": "background.js"
  },

  "permissions": ["tabs"],
  "action": {
    "default_title": "Cucumber"
  }
}
